<p>
  <strong><%= t('.title')%>:</strong>
  <%= @test.title %>
</p>

<p>
  <strong><%= t('.level')%>:</strong>
  <%= @test.level %>
</p>

<p>
  <strong><%= t('.category')%>:</strong>
  <%= @test.category.title %>
</p>
<table>

  <tr>
    <th><%= t('.questions')%>:</th>
    <th colspan="3"><%= t('.actions')%></th>
  </tr>
    <%= render partial: 'admin/questions/question', collection: @test.questions %>
</table>

<p>
  <%= link_to t('.edit_test', test: @test.title), edit_admin_test_path(@test) %><br>
  <%= link_to t('.create_question'), new_admin_test_question_path(@test) %><br>
  <%= link_to t('.back_to_tests'), admin_tests_path %><br>
</p>
